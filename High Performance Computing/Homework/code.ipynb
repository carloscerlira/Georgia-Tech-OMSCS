    "def reduce(A):\n",
    "    n = len(A)\n",
    "    print(A)\n",
    "    if n <= 2:\n",
    "        a = reduce(A[0:n/2])\n",
    "        b = reduce(A[n/2:])\n",
    "        return a+b\n",
    "    else:\n",
    "        return A[0]"
   ]
