    "def reduce(A):\n",
    "    n = len(A)\n",
    "    if n >= 2:\n",
    "        a = reduce(A[0:int(n/2)])\n",
    "        b = reduce(A[int(n/2):])\n",
    "        return a+b\n",
    "    else:\n",
    "        return A[0]"
   ]
